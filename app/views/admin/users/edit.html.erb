<%= link_to "Back Admin Panel", admin_users_path %> 
<%= form_for(@admin_user, url: admin_user_path(@admin_user)) do |f| %>
  <div>
    <%= f.label "firstname" %><br>
    <%= f.text_field "firstname" %>
  </div>
  <div>
    <%= f.label "lastname" %><br>
    <%= f.text_field "lastname" %>
  </div>
  <div>
    <%= f.label "email" %><br>
    <%= f.text_field "email" %>
  </div>

  <div>
    <%= f.submit "Submit" %> 
  </div>
<% end %>
