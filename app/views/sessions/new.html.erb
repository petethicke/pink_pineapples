<div class="main">
  <h1>Log In</h1>
  <%= form_tag session_path do %>
  <div class="main">
    <%= label_tag "email" %><br/>
    <%= text_field_tag "email" %>
  </div>
  <div class="main">
    <%= label_tag :password %><br/>
    <%= password_field_tag :password %>
  </div>
  <div class="main"><%= submit_tag "Log In" %></div>
  <% end %>
</div>